#构建
docker build -t dockerhub:5000/logstash --rm=true .
#启动
docker run --name=logstash -p 9600:9600 -v /home/docker/log/nginx:/home/docker/log/nginx -ti -d dockerhub:5000/logstash
docker run --restart=always --name=logstash -p 9600:9600 -v /home/logstash/config:/home/logstash/config -v /home/logstash/logs:/home/logstash/logs -v /home/docker/log/nginx:/home/docker/log/nginx -ti -d dockerhub:5000/logstash
